{
  "manifest_version": 3,
  "name": "PromptKeeper",
  "version": "1.0.0",
  "description": "Clipboard and storage dedicated to commonly used prompts for quick access through any LLM agents",
  "permissions": [
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}